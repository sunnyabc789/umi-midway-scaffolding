<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no" />
    <% if (context.title != null) { %>
        <title><%= context.title %></title>
    <% } %>
    <script>
      try {
        window.context_env = <%- JSON.stringify(context.env || 'development') %>;
        window.JWT = <%- JSON.stringify(context.jwt || '') %>;
        window.sugo = <%- context.sugo || '""' %>;
      } catch (e) {
        console.warn(e)
      }
  </script>
</head>
<body>

<div id="<%= context.config.mountElementId %>" class="sugo-portal-app"></div>

</body>
</html>
